<?xml version="1.0" encoding="utf-8"?>
<!-- Background for empty save slots - dashed border -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <solid android:color="@color/rm_slot_empty_bg" />
    <stroke
        android:width="1dp"
        android:color="@color/rm_divider"
        android:dashWidth="4dp"
        android:dashGap="2dp" />
    <corners android:radius="8dp" />
</shape>
